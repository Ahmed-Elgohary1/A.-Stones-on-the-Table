#include <bits/stdc++.h>
    using namespace std;
    const int N=100020;
    int a[N]={0};
    int main()
    {
        int n;
        scanf("%d",&n);
        int x=n;
        for(int i=0;i<n;i++)
        {
            int p;
            cin>>p;
            a[p]=1;
            while(a[x]==1)
            {
                printf("%d ",x);
                x--;
            }
            printf("\n");
        }
        return 0;
    }
