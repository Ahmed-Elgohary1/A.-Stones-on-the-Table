#include <iostream>

using namespace std;

int main()
{
int n,j=0;
string x;
cin>>n;

for(int i=0;i<n;i++)
{
cin>>x;
   if(x== "Tetrahedron")
   {
       j=j+4;
   }
    else if(x== "Cube")
   {
       j=j+6;
   }
   else if(x== "Octahedron")
   {
       j=j+8;
   }
   else if(x== "Dodecahedron")
   {
       j=j+12;
   }
   else if(x== "Icosahedron")
   {
       j=j+20;
   }



}
cout<<j;
    return 0;
}
